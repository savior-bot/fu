{
    "manifest_version": 3,
    "name": "YouTube Content Filter",
    "version": "1.0",
    "description": "Filter YouTube content for a distraction-free experience",
    "permissions": ["storage"],
    "background": {
      "service_worker": "Background.js",
      "type": "module"
    },
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": {
        "16": "images/icon-16.png",
        "48": "images/icon-32.png",
        "128": "images/icon-128.png"
      }
    },
    "icons": {
      "16": "images/icon-16.png",
      "48": "images/icon-32.png",
      "128": "images/icon-128.png"
    }
}
  